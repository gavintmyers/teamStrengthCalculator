<!DOCTYPE html>
<html>
<head>
  <title>TeamStrengthCalculator</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <nav class="navbar navbar-default">
    <div class="container-fluid">
      <div class="navbar-header">
        <div class="navbar-brand">Team Strength Calculator</div>
        <button class="navbar-toggle hidden-sm-up" type="button" data-toggle="collapse" data-target="#navi">
        &#9776;
        </button>
      </div>
      <div class="collapse navbar-collapse" id="navi">
        <ul class="nav navbar-nav navbar-right">
          <li><%= link_to 'Sprints', sprints_path %></li>
          <li><%= link_to 'Developers', developers_path %></li>
          <li><%= link_to 'Clients', root_path %></li>
          <li><%= link_to 'Hours', root_path %></li>
        </ul>
      </div>
    <div>
  </nav>
<%= yield %>

</body>
</html>
